{
    "name": "Table of Contents",
    "version": "1.0",
    "description": "Adds a table of contents to the website for easier navigation",
    "manifest_version": 2,
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [{
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content.js"],
      "css": ["myStyle.css"],
      "run_at": "document_idle"
    }]
  }
  